{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:webview:allow-create-webview-window",
    "dialog:default",
    "http:default",
    "os:default",
    "fs:default",
    {
      "identifier": "fs:allow-exists",
      "allow": [{ "path": "$APPDATA/*" }]
    },
    "opener:default",
    "process:default",
    "shell:default"
  ]
}